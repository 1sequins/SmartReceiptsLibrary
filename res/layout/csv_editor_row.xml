<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/row"
	android:layout_width="match_parent"
	android:layout_height="wrap_content" >
	<TextView
		android:id="@+id/left"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/cell"
		android:paddingTop="@dimen/table_padding"
		android:paddingBottom="@dimen/table_padding"
		android:paddingRight="@dimen/table_col1_side_padding"
		android:paddingLeft="@dimen/table_col1_side_padding"
		android:gravity="center"
		android:textSize="@dimen/table_textSize" />
	<Spinnger
		android:id="@+id/right"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="@drawable/cell"
		android:padding="@dimen/table_padding"
		android:gravity="center"
		android:textSize="@dimen/table_textSize" />
</TableRow>